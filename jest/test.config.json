{
  "moduleFileExtensions": ["js"],
  "moduleDirectories": ["node_modules"],
  "testRegex": ".*\\.test\\.js$",
  "testPathIgnorePatterns": ["<rootDir>/scaffold/"],
  "coveragePathIgnorePatterns": ["<rootDir>/scaffold/", "<rootDir>/node_modules/"],
  "collectCoverageFrom": [
    "<rootDir>/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
