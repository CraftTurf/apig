{
  "name": "@ctt/apig",
  "version": "0.1.0",
  "description": "A crud-api microservice and endpoint generator",
  "main": "bin/apig",
  "bin": {
    "apig": "bin/apig"
  },
  "scripts": {
    "build": "babel src -d lib --ignore *.test*.js",
    "build:watch": "yarn build --watch",
    "clean": "concurrently 'rimraf node_modules' 'rimraf lib/*' 'yarn cache clean'",
    "lint": "eslint src",
    "prepublish": "yarn build",
    "pretest": "yarn lint",
    "start": "node lib/index.js",
    "test": "jest --config jest/test.config.json",
    "test:watch": "yarn test --watch",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "files": [
    "bin",
    "lib",
    "scaffold"
  ],
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-core": "6.4.5",
    "babel-eslint": "6.1.2",
    "babel-jest": "17.0.2",
    "babel-plugin-transform-class-properties": "6.4.0",
    "babel-plugin-transform-es2015-spread": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.3.13",
    "concurrently": "3.5.1",
    "eslint": "3.12.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "21.15.0",
    "jest": "17.0.3",
    "precommit-hook-eslint": "3.0.0"
  },
  "dependencies": {
    "capitalize": "2.0.0",
    "inquirer": "6.2.1",
    "mkdirp": "0.5.1",
    "mustache": "3.0.1",
    "ramda": "0.26.1",
    "scaffold-generator": "2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jabdul/apig.git#master"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "team@craftturf.com",
  "license": "SEE LICENSE IN <LICENSE>"
}
