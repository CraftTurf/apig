{
  "name": "{{{scaffold_project_name}}}",
  "version": "0.0.1",
  "description": "{{{scaffold_project_description}}}",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib --ignore *.test*.js",
    "dev": "nodemon",
    "dump": "babel-node -- src/persistence/mongoose/backup.js",
    "clean": "concurrently 'rimraf node_modules' 'rimraf lib/*' 'yarn cache clean'",
    "flow": "flow check --all",
    "flow:dep": "flow-typed install",
    "lint": "eslint src",
    "postinstall": "[ \"$NODE_ENV\" == production ] && exit 0; yarn flow:dep",
    "prepublish": "yarn build",
    "pretest": "yarn lint",
    "seed": "babel-node -- src/persistence/mongoose/seed.js",
    "start": "node lib/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "yarn test --watch",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "roots": [
      "./src/"
    ],
    "testMatch": [
      "**/?(*.)test.js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "babel-polyfill",
      "./test/utils/consoleErrorsThrowException.js"
    ],
    "setupFilesAfterEnv": ["<rootDir>/test/utils/testInitializers.js"]
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel-eslint": "8.1.1",
    "babel-jest": "17.0.2",
    "concurrently": "3.6.1",
    "database-cleaner": "1.3.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.20.2",
    "factory-girl": "5.0.4",
    "flow-bin": "0.78.0",
    "flow-typed": "2.5.1",
    "jest": "24.9.0",
    "nodemon": "^2.0.1",
    "precommit-hook-eslint": "3.0.0"
  },
  "dependencies": {
    "@ctt/crud-api": "*",
    "@ctt/service-utils": "*",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/plugin-transform-flow-strip-types": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-flow": "7.8.3",
    "@babel/runtime": "7.8.4",
    "convict": "4.3.2",
    "dotenv": "6.0.0",
    "hapi-auth-jwt2": "8.1.0",
    "mongodb-backup": "1.6.9",
    "mongodb-restore": "1.6.2",
    "mongoose": "5.3.4",
    "mongoose-paginate": "5.0.3",
    "ramda": "0.25.0",
    "uuid": "3.3.2",
    "vision": "5.3.3"
  },
  "engines": {
    "node": ">=10.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://{{{scaffold_project_git_url}}}"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "{{{scaffold_project_author}}}",
  "license": "SEE LICENSE IN <LICENSE>"
}
